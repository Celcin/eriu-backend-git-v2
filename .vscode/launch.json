{
	"version": "0.2.0",
	"configurations":
	[
		{
			"name": "Listen for Xdebug (Symfony)",
			"type": "php",
			"request": "launch",
			"port": 9003,
			"pathMappings":
			{
				"/app": "${workspaceFolder}"
			},
			"log": true,
			"xdebugSettings":
			{
				"max_children": 128,
				"max_data": 512,
				"max_depth": 5
			}
		}
	]
}
